// runner.8g - 八百万エンジンの実行制御

モジュール 起動装置 = {
    // Rustの「実行する」に相当
    処理 実行する(ソースコード) {
        // mod.8g (全体の統括) の実行関数を呼び出す
        戻す 外部::実行(ソースコード)
    }

    // Rustの「ファイルを読み込んで実行」に相当
    処理 ファイルを実行する(パス) {
        もし (ファイル::存在確認(パス)) {
            変数 コード = ファイル::読み込み(パス)
            戻す この::実行する(コード)
        } さもなくば {
            戻す "❌ ファイル「" + パス + "」が見つかりませんでした。"
        }
    }
}
